{
	"name": "dynamimg-new",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
